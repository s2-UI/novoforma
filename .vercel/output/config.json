{
	"version": 3,
	"routes": [
		{
			"src": "^/_assets/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/sendEmail\\.json\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/conditions-use\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/privacity\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/sitemap\\.xml\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "_render",
			"status": 404
		}
	]
}